      <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="ALWAYS" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="192.0" minHeight="50.0" prefHeight="192.0" vgrow="ALWAYS" />
          <RowConstraints maxHeight="350.0" minHeight="208.0" prefHeight="208.0" vgrow="ALWAYS" />
        </rowConstraints>
         <children>
            <TableView fx:id="statsTable" GridPane.rowIndex="1" />
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" />
                <ColumnConstraints hgrow="ALWAYS" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="ALWAYS" />
              </rowConstraints>
               <children>
                  <Accordion fx:id="statsAccordion" prefHeight="192.0" prefWidth="0.0" GridPane.columnIndex="1" />
               </children>
            </GridPane>
         </children>
      </GridPane>
   </children>
</fx:root>
